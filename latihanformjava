    private void submitActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_submitActionPerformed
        // TODO add your handling code here:
        String name = tf_name.getText();
        System.out.println(name);
        
        ModelProgram mp = new ModelProgram();
        mp.setName(name);
        
        NameLogic logic = new NameLogic();
        String result = logic.returnName(mp);
        
        NameLogic logic1 = new NameLogic();
        String resulti = logic.returnNull(mp);
        
        if (tf_name.getText().length()==0){
            label_result.setText(resulti);
        }
        else {
            label_result.setText(result);
        }
    }//GEN-LAST:event_submitActionPerformed

